    if( $user_id['user_id'] == $user->uid ) {

      // can bay
      $arr1 = db_fetch_row( "SELECT COUNT( 1 ) cnt FROM trade_in WHERE object_id = $oid" );
      $arr2 = db_fetch_row( "SELECT COUNT( 1 ) cnt FROM trade_out WHERE object_id = $oid" );
      $arr3 = db_fetch_row( "SELECT COUNT( 1 ) cnt FROM warehouse WHERE place_type IN ( 0, 1, 2 ) AND object_id = $oid" );
  
      if( $arr1['cnt'] > 0 || $arr2['cnt'] > 0 || $arr3['cnt'] > 0 ) {
        $out .= "&canbay=1";
