                         INNER JOIN objects o ON o.id = os.res_id AND o.class IN( 1, 2, 9 )
                         INNER JOIN objects o1 ON o1.id = p.res_id
                              WHERE p.event_id = $event_id" );
  
    foreach( $prod as $prod_item ) {
      if( $prod_item['res_id'] == 1043 ) {
        levelUp( 16, $disp['user_id'] );
