      add_warehouse_item( 1, $planet_id, $prod_item['res_id'], $prod_item['cnt'] );
    }
  
    send_msg( 1, $disp['user_id'], t( "œÂÂ‡·ÓÚÍ‡ Ï‡ÚÂË‡Î‡ <font color=\"#00FFFF\">'%s'</font> ‚ ÒËÒÚÂÏÂ %s Ì‡ ÔÎ‡ÌÂÚÂ <font color=\"#00FFFF\">%s (%d:%d)</font> Á‡‚Â¯ÂÌ‡ ÛÒÔÂ¯ÌÓ.", array( $disp['name'], $disp['sname'], $disp['pname'], $disp['x'], $disp['y'] ) ), 4, $disp['event_time'] );
  
