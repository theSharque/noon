#ifndef __IOINIT_C__
#define __IOINIT_C__

/*
* initializes IO ports
* used for GenBoard
*/

// #include "edis.c"
/*!
 * \ingroup g_init
 * \brief I/O Port Initialization for power-up.
 *
 * \return none
 */

static inline void ioinit(void)
{
#if (defined __MEGASQUIRT_C__) || (! defined HARDWARE_PULLUP_OR_DOWN)
// when called from megasquirt.c we always set output
// note: firmware can be uploaded easier then bootloader!

// For bootloader, HARDWARE_PULLUP_OR_DOWN means that
// the SIL5 resistor networks installed and R154 or R153
// is used depending on FETDRIVER (inverting or not)
// than we do nothing: leave the port in high impedance (input)
#ifdef FETDRIVER_INVERTING
	PORTA = 0xFF;
#else
	PORTA = 0x00; // no pullup for injectors
#endif // FETDRIVER_INVERTING
	DDRA = 0xFF;
#endif // HARDWARE_PULLUP_OR_DOWN

	// SS high
	PORTB = 0x01;
	DDRB = 0xF7;

	// CS_KNOCK high
	PORTC = 0x04;
	DDRC = 0xFF;	// a bad LCD_DATADDR30 can screw this !

	// CS_S259, CS_P259, CS_CAN are high
#ifdef FORCE_TRIGGER_IC1_PULLUP
	PORTD = 0xF3;   // pullup on IC1 (0x10) on
#else
	PORTD = 0xE3;	// pullup on IC1 (0x10) off
#endif
	DDRD = 0xE8; // PD0 and PD1 (old ALS, LAUNCH) are always input with pullup. Also used for stim-enable

#ifdef FORCE_TRIGGER_IC3_PULLUP
	PORTE = 0x80; // secondary trigger PE7=IC3 pullup on
#else
	PORTE = 0x00; // secondary trigger PE7=IC3 pullup off
#endif	// TRIGGER_PULLUP_OFF

	DDRE = 0x3A;

	PORTF = 0x00; // adc channels
	DDRF = 0x00;

	// outputs CS_MCP(4), CS_IGN259(2) are high, HEAT2(1), HEAT1(0) are low:
	// inputs CS_FLASH(3)
	PORTG = 0x1C;
	DDRG = 0x17;
