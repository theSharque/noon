      <component>
         <name>Mombi</name>
         <helper>
            <name/>

            <events>
                <!--    =========================================    -->
                <!--                DMCInstallTime                   -->
                <!--     called during DMC Install Time (Import)     -->
                <!--    =========================================    -->

                <event>
                    <name>DMCInstallTime</name>
                    <macro_name>macro_PrintQualityImport</macro_name>
                </event>

                <!--    =========================================    -->
                <!--                DMCUpgradeTime                   -->
                <!--     called during DMC Upgrade Time (Export)     -->
                <!--    =========================================    -->

                <event>
                    <name>DMCUpgradeTime</name>
                    <macro_name>macro_PrintQualityExport</macro_name>
                </event>

            </events>
        
            <!-- put a name if we have multiple helpers-->
            <features>
            <!-- ========================================= -->
            <!--     Emulate LaserJet Margins              -->
            <!-- ========================================= -->
                <feature>
                <name>HP_Emulate_Margins</name>
                <switch>
                <name>Paper_Dimensions</name>
               <case_default>
                      <data>200_200_200_200</data>
               </case_default>
               </switch>
               </feature>


            <!--  Features are Paper size, Media Type and Print Quality -->
             <!-- =========================================== -->
             <!--                  Paper Size                 -->
             <!-- =========================================== -->
             <feature>
                 <name>PaperSize</name>
                 <macro_name>macro_PaperSize</macro_name>
             </feature>
               <!-- ========================================= -->
               <!--                 MediaType                 -->
               <!-- ========================================= -->
               <feature>
                  <name>MediaType</name>
                  <macro_name>macro_MediaType</macro_name>
               </feature>

              <feature>
                  <name>HPPhotoFix</name>
                  <macro_name>macro_HPPhotoFix</macro_name>
               </feature>

            <feature>

                  <name>Resolution</name>

                  <macro_name>macro_resolution</macro_name>

            </feature>

 
               <!-- ========================================= -->
               <!--           Print Quality Group             -->
               <!--   Options are as follows                  -->
               <!--   (a) PQGroup_1 - Best                    -->
               <!--   (b) PQGroup_2 - Normal                  -->
               <!--   (c) PQGroup_3 - FastDraft                    -->
               <!--   (d) PQGroup_4 - Fast Normal             -->
               <!--   (e) PQGroup_5 - 4800x1200             -->
               <!-- ========================================= -->
               <feature>
                  <name>PrintQualityGroup</name>
                  <macro_name>macro_PrintQualityGroup</macro_name>
               </feature>
               <feature>
                  <name>HPInputColorSpace</name>
                  <macro_name>macro_HPInputColorSpace</macro_name>
               </feature>
              <feature>
                <name>CustomSize</name>
                <macro_name>macro_custom_size</macro_name>            
              </feature>
              <!--                                             -->
               <!--            ICM Profile FileName macro       -->
               <!-- ========================================= -->
              <feature>
                 <name>ICCProfileFileName</name>
                 <macro_name>macro_ICCProfileFileName</macro_name>
             </feature>
             <feature>
                 <name>InstalledPenSet_Premium</name>
                 <macro_name>macro_InstalledPenSet_Premium</macro_name>
             </feature>
            
             <feature>
                 <name>InstalledPenSet_PremiumPlusPhoto</name>
                 <macro_name>macro_InstalledPenSet_PremiumPlusPhoto</macro_name>
             </feature>        
            </features>
            <macros>
               
               <!-- ========================================= -->
               <!--                HPInput ColorSpace         -->
               <!--                                           -->
               <!-- ========================================= -->
               
           
           <macro>

                  <name>macro_Resolution</name>

                  <switch>

                     <name>Resolution</name>
                     <case>
                        <name>300dpi</name>                     
                          <select_option>PrintQualityGroup.PQGroup_3</select_option>  
                         <select_option>PrintQuality.FastDraft</select_option>                        
                    </case>
                    <case>
                        <name>600dpi</name>                     
                          <select_option>PrintQualityGroup.PQGroup_1</select_option> 
                          <select_option>PrintQuality.Best</select_option>                         
                    </case>
                    <case>
                        <name>1200dpi</name>                     
                          <select_option>PrintQualityGroup.PQGroup_5</select_option>  
                          <select_option>PrintQuality.4800x1200</select_option>                       
                    </case>
                    </switch>
                     <call>macro_PQ_PhotoFix</call>
                </macro>

           
           <macro>
              <name>macro_HPInputColorSpace</name>
              <switch>
                 <name>HPInputColorSpace</name>
                 <case>
                    <name>ADOBERGB</name>
                                        
                    <switch>
                        <name>MediaType</name>
                            <case>
                                <name>HP_CD_TATTOO_PAPER</name>
                                <enable_option>PrintQualityGroup.PQGroup_1</enable_option>
                                <enable_option>PrintQualityGroup.PQGroup_2</enable_option>
                                <switch>
                                    <name>HPMaxDpi</name>
                                    <case>
                                        <name>False</name>
                                        <hide_option>PrintQualityGroup.PQGroup_5</hide_option>
                                    </case>
                                    <case>
                                        <name>True</name>
                                        <enable_option>PrintQualityGroup.PQGroup_5</enable_option>
                                    </case>
                                </switch>
                                <hide_option>PrintQualityGroup.PQGroup_3</hide_option>
                            </case>                
                    </switch>
                   <switch>
                     <name>PaperSize</name>
                     <case>
                       <name>13_X_18_CM</name>
                       <select_option>PaperSize.13_X_18_CM</select_option>
                     </case>
                     <case_default>
                       <select_option>PaperSize.5_X_7_IN</select_option>
                     </case_default>
                   </switch>
                    <resource_id>3996</resource_id>
                    <resource_id>3996</resource_id>
                    <resource_id>968</resource_id>
                 </case>
                 <case>
                    <name>MANAGEDBYAPPLN</name>
                    <select_option>PrintQualityGroup.PQGroup_1</select_option>
                    <resource_id>3996</resource_id>
                    <resource_id>3996</resource_id>
                    <resource_id>968</resource_id>
                 </case>
                
                 <case_default>
                     
                     <enable_option>PrintQualityGroup.PQGroup_1</enable_option>
                                <enable_option>PrintQualityGroup.PQGroup_2</enable_option>
                     <switch>
                         <name>HPMaxDpi</name>
                         <case>
                             <name>False</name>
                             <hide_option>PrintQualityGroup.PQGroup_5</hide_option>
                         </case>
                         <case>
                             <name>True</name>
                             <enable_option>PrintQualityGroup.PQGroup_5</enable_option>
                         </case>
                     </switch>
                                <enable_option>PrintQualityGroup.PQGroup_3</enable_option>
                     <resource_id>3996</resource_id>
                     <resource_id>3996</resource_id>
                     <resource_id>968</resource_id>
                 </case_default>
              </switch>
           </macro>

            <macro>
                  <name>macro_HPPhotoFix</name>
                         <switch>
                            <name>HPPhotoFix</name>
                                <case>
                                    <name>OFF</name>
                                    <select_option>HPSmartFocus.Off</select_option>
