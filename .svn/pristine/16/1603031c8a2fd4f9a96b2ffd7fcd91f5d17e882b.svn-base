server {
	listen			3131;

	server_name		21noon.com 5.9.140.7;
	server_tokens		off;
	root			/var/cache/munin/www;

	auth_basic		"Enter your password";
	auth_basic_user_file	/etc/nginx/passwd;

	ssl			on;
	ssl_certificate		/etc/nginx/ssl/noon.com.crt;
	ssl_certificate_key	/etc/nginx/ssl/noon.com.key;

	add_header              Front-End-Https    on;

	location / {
	    index index.html;
	    access_log /var/log/nginx/munin.access.log ;
	    error_log /var/log/nginx/munin.error.log ;
	}

	location ^~ /cgi-bin/munin-cgi-graph/ {
		access_log off;
		include fastcgi_params;
		fastcgi_split_path_info ^(/cgi-bin/munin-cgi-graph)(.*);
		fastcgi_param PATH_INFO $fastcgi_path_info;
		fastcgi_pass unix:/var/run/munin/munin-fastcgi-graph.sock;
	}
}
