                                    <select_option>HPDigitalFlash.On</select_option>
                                    <select_option>HPSharpness.Low</select_option>
                                    <select_option>HPSmoothing.On</select_option>
                                </case>
                                <case>
                                    <name>FULL</name>
                                    <select_option>HPSmartFocus.On</select_option>
                                    <select_option>HPContrast.Off</select_option>
                                    <select_option>HPDigitalFlash.Off</select_option>
                                    <select_option>HPSharpness.Off</select_option>
                                    <select_option>HPSmoothing.On</select_option>
                                </case>
                    </switch>
         </macro>


