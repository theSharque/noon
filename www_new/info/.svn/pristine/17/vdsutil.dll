*Feature: PaperSize
{
    *Option: 3_5_X_5
    {
        *rcNameID: =IDS_MSIZE_3_5X5_IN
        *PageDimensions: PAIR(4200,6000)
        *Constraints: HPPrintOnBothSidesManually.True
        *Command: CmdSelect
        {
            *Order: PAGE_SETUP.20
            *Cmd: =PCL_PHOTO_L
        }
        *switch: HPMinimizeMargins
        {
            *case: True
            {
                *switch: Orientation
                {
                    *case: PORTRAIT
                    {
                        *PrintableArea: PAIR(3872,5700)
                        *PrintableOrigin: PAIR(164,150)
                        *CursorOrigin: PAIR(164,150)
                    }
                    *case: LANDSCAPE_CC270
                    {
                        *PrintableArea: PAIR(3872,5700)
                        *PrintableOrigin: PAIR(164,150)
                        *CursorOrigin: PAIR(4036,150)
                    }
                }
            }
            *case: False
            {
                *switch: HPReportSymmetricMargins
                {
                    *case: False
                    {
                        *switch: Orientation
                        {
                            *case: PORTRAIT
                            {
                                *PrintableArea: PAIR(3872,5696)
                                *PrintableOrigin: PAIR(164,150)
                                *CursorOrigin: PAIR(164,150)
                            }
                            *case: LANDSCAPE_CC270
                            {
                                *PrintableArea: PAIR(3872,5696)
                                *PrintableOrigin: PAIR(164,150)
                                *CursorOrigin: PAIR(4036,150)
                            }
                        }
                    }
                    *case: True
                    {
                        *switch: Orientation
                        {
                            *case: PORTRAIT
                            {
                                *PrintableArea: PAIR(3872,5692)
                                *PrintableOrigin: PAIR(164,154)
                                *CursorOrigin: PAIR(164,150)
                            }
                            *case: LANDSCAPE_CC270
                            {
                                *PrintableArea: PAIR(3872,5692)
                                *PrintableOrigin: PAIR(164,154)
                                *CursorOrigin: PAIR(4036,150)
                            }
                        }
                    }
                }
            }
        }
    }
    *Option: HP_BORDERLESS_3_5_X_5
    {
        *rcNameID: =IDS_MSIZE_BORDERLESS_3_5_5
        *Constraints: HPPrintOnBothSidesManually.True
        *PageDimensions: PAIR(4200,6000)
        *Command: CmdSelect
        {
            *Order: PAGE_SETUP.20
            *Cmd: =PCL_PHOTO_L
        }
        *switch: Orientation
        {
            *case: PORTRAIT
            {
                *PrintableArea: PAIR(4200,6000)
