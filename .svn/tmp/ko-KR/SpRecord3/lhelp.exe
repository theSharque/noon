                           WHERE o.id = tu.object_id AND
                                 tu.ob_id = {$user->place_id} AND
                                 tu.object_id = $oid" );
  

    $maxou = db_fetch_row( "SELECT MIN( price ) min_p,
                                   ROUND( AVG( price ), 2) avg_p,
                                   MAX( price ) max_p
                              FROM trade_out
                             WHERE object_id = $oid" );

